package com.seintes.wanandroid.project;

import com.seintes.wanandroid.net.ArticleDatas;

import java.util.ArrayList;
import java.util.List;

/**
 * @Description 导航栏中的一级目录
 * @Author xmhaz
 * @Time 2023/12/14 15:37
 */
public class NaviTab {
    private List<SimpleActicle> articles= new ArrayList<>();
    private int cid;
    private String name;

    public List<SimpleActicle> getArticles() {
        return articles;
    }

    public void setArticles(List<SimpleActicle> articles) {
        this.articles = articles;
    }

    public int getCid() {
        return cid;
    }

    public void setCid(int cid) {
        this.cid = cid;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "NaviTab{" +
                "articles=" + articles +
                ", cid=" + cid +
                ", name='" + name + '\'' +
                '}';
    }
}
